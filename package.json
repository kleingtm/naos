{
	"name": "tns-ts-vue",
	"version": "0.1.0",
	"private": true,
	"nativescript": {
		"id": "org.nativescript.tns-ts-vue",
		"tns-android": {
			"version": "4.2.0"
		},
		"tns-ios": {
			"version": "4.2.0"
		}
	},
	"scripts": {
		"serve": "vue-cli-service serve",
		"build:ios": "PLATFORM=ios vue-cli-service build",
		"debug:ios": "TNS_CMD=debug PLATFORM=ios shx rm -rf dist && vue-cli-service $NODE_DEBUG_OPTION build --mode development",
		"debug:android": "PLATFORM=android vue-cli-service build",
		"watch:ios": "PLATFORM=ios TNS=watch shx rm -rf dist && vue-cli-service $NODE_DEBUG_OPTION build --mode development",
		"inspect": "PLATFORM=ios vue inspect",
		"launch": "node $NODE_DEBUG_OPTION utils/launch.js",
		"lint": "vue-cli-service lint",
		"lint:fix": "node_modules/.bin/tslint --project tsconfig.json --fix",
		"lint:check": "node_modules/.bin/tslint-config-prettier-check ./tslint.json",
		"prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
		"pre-commit": "yarn lint:fix && yarn prettier",
		"postinstall": "node -e \"process.env.npm_execpath.includes('yarn') || require('child_process').execSync('rm -rf package-lock.json', {stdio:[0,1,2]});\"",
		"test:unit": "vue-cli-service test:unit",
		"test:e2e": "vue-cli-service test:e2e",
		"clean": "rm -rf dist node_modules package-lock.json yarn.lock"
	},
	"dependencies": {
		"nativescript-theme-core": "~1.0.4",
		"tns-core-modules": "~4.2.0",
		"vue": "^2.5.17",
		"vue-class-component": "^6.0.0",
		"vue-property-decorator": "^7.0.0",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@types/jest": "^23.1.4",
		"@vue/cli-plugin-babel": "^3.0.0",
		"@vue/cli-plugin-e2e-nightwatch": "^3.0.0",
		"@vue/cli-plugin-typescript": "^3.0.0",
		"@vue/cli-plugin-unit-jest": "^3.0.0",
		"@vue/cli-service": "^3.0.0",
		"@vue/test-utils": "^1.0.0-beta.20",
		"babel-core": "7.0.0-bridge.0",
		"cssnano": "^4.0.5",
		"nativescript-vue-externals": "^0.2.0",
		"nativescript-vue-loader": "^0.1.5",
		"nativescript-vue-target": "^0.1.0",
		"nativescript-vue-template-compiler": "^1.3.1",
		"node-sass": "^4.9.3",
		"ns-vue-loader": "^0.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.0",
		"sass-loader": "^7.0.1",
		"shelljs": "^0.8.2",
		"shx": "^0.3.2",
		"ts-jest": "^23.0.0",
		"tslint-config-prettier": "^1.15.0",
		"typescript": "^3.0.1",
		"vue-template-compiler": "^2.5.17",
		"webpack": "^4.17.1",
		"webpack-node-externals": "^1.7.2",
		"webpack-synchronizable-shell-plugin": "^0.0.7",
		"winston-color": "^1.0.0"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"engines": {
		"node": ">=6.0.0"
	}
}
